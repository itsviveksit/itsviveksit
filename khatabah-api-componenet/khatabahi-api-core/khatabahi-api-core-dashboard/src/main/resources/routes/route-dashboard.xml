<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd
    ">

    <!-- this is an included XML file where we only the the routeContext -->
    <routeContext id="dashboardRouteContext" xmlns="http://camel.apache.org/schema/spring">
        <!-- we can have a route -->
        <route id="dashboardRouteContextId">
            <from uri="{{1fa.v1.ccs_dashboard_v1_details}}"/>
            <log message="start dashboard ms calling"></log>
            <process ref="dashboardAccountDetailsProcessor"></process>
            <log message="finish dashboard ms calling"></log>
        </route>

    </routeContext>

</beans>
